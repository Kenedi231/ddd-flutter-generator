{
	"name": "ddd-flutter-generator",
	"displayName": "DDD-Flutter-Generator",
	"description": "Generate DDD",
	"version": "0.0.1",
	"engines": {
		"vscode": "1.44.2"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ddd-flutter-generator.initialize",
		"onCommand:ddd-flutter-generator.createscreen"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ddd-flutter-generator.initialize",
				"title": "DDD Flutter Generator: Initialize"
			},
			{
				"command": "ddd-flutter-generator.createscreen",
				"title": "DDD Flutter Generator: Create screen"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "7.1.3",
		"@types/js-yaml": "^3.12.5",
		"@types/lodash": "^4.14.166",
		"@types/mocha": "8.0.4",
		"@types/node": "12.11.7",
		"@types/vscode": "1.47.0",
		"@typescript-eslint/eslint-plugin": "4.9.0",
		"@typescript-eslint/parser": "4.9.0",
		"eslint": "7.15.0",
		"glob": "7.1.6",
		"js-yaml": "^3.14.1",
		"lodash": "4.17.21",
		"mocha": "8.1.3",
		"path": "^0.12.7",
		"ts-loader": "8.0.11",
		"typescript": "4.1.2",
		"vscode-test": "1.4.1"
	},
	"dependencies": {
		"@types/shelljs": "^0.8.8",
		"child_process": "^1.0.2",
		"shelljs": "^0.8.4"
	}
}
